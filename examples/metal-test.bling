
# metal

filter mitchell 3 3 0.333333 0.333333
filter box

imageSize 640 400
# imageSize 1280 720

renderer {
   sampler
   sampled {
      sampler { stratified 2 2 }
      integrator { path maxDepth 7 sampleDepth 3 }
   }
}

# renderer { metropolis mpp 0.5 bootstrap 500 plarge 0.25 }

transform {
   lookAt {
      pos -2 12 -12
      look 0 -2 0
#      look -5 2 0
      up 0 1 0
   }
}

camera {
   perspective
   fov 45
   lensRadius 0
   focalDistance 10
}

